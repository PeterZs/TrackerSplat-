{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "init 1st frame",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplat.initialize",
            "console": "integratedTerminal",
            "args": [
                "-d",
                "data/walking/frame60",
                "--initializer",
                "colmap-dense",
                "-o",
                "colmap_executable='./data/colmap/COLMAP.bat'",
            ]
        },
        {
            "name": "train 1st frame",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplat.train",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "data/walking/frame60",
                "-d",
                "output/walking/frame60",
                "-i",
                "1000",
            ],
            "justMyCode": false
        },
        {
            "name": "dot motionestimation",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.motionestimation",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/walking",
                "-d",
                "output/walking",
                "-i",
                "1000",
                "--pipeline",
                "train/regularized",
                // "train/base",
                // "track/propagate-dot-cotracker3",
                // "track/filter-dot-cotracker3",
                // "track/base-dot-cotracker3",
                "--iteration_init",
                "1000",
                "--start_frame",
                "60",
                "-n",
                "9",
                "-b",
                "10",
                "--tracking_rescale",
                "0.3",
            ]
        },
        {
            "name": "sam2 featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--load_camera",
                "output/truck/cameras.json",
                "--extractor",
                "sam2",
                "--extractor_configfile",
                "./configs/sam2.1/sam2.1_hiera_l.yaml",
                "--extractor_checkpoint",
                "./checkpoints/sam2.1_hiera_large.pt",
                "--save_featuremap",
                "--colorify_algo",
                "kmeans-pca",
                "--colorify_alpha",
                "1.0",
            ]
        },
        {
            "name": "dinov2 featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--load_camera",
                "output/truck/cameras.json",
                "--extractor",
                "dinov2",
                "--extractor_configfile",
                "./configs/dinov2/vits14_reg4_pretrain.yaml",
                "--extractor_checkpoint",
                "./checkpoints/dinov2_vits14_reg4_pretrain.pth",
                "--save_featuremap",
                "--colorify_algo",
                "kmeans-pca",
                "--colorify_alpha",
                "0.8",
            ]
        },
        {
            "name": "dinov2seg featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/stepin/frame60",
                "-d",
                "output/stepin/frame60",
                "-i",
                "1000",
                "--mode",
                "densify",
                "--load_camera",
                "output/stepin/frame60/cameras.json",
                "--extractor",
                "dinov2seg",
                "--extractor_configfile",
                "large",
                "--save_featuremap",
                "--colorify_algo",
                "weightedsum",
            ]
        },
        {
            "name": "point tracking",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.pointtracking",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/walking",
                "-d",
                "output/walking",
                "--estimator",
                "dot-cotracker3",
                "--start_frame",
                "60",
                "-n",
                "10",
                "--tracking_rescale",
                "0.5",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "render",
            "type": "debugpy",
            "request": "launch",
            "module": "gaussian_splatting.render",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/walking/frame69",
                "-d",
                // "output/walking/frame69",
                "output/walking/train/regularized/frame69",
                "-i",
                "1000",
                "--mode",
                "pure",
            ]
        },
        {
            "name": "incremental training",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.incrementaltraining",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/walking/frame69",
                "-d",
                "output/walking/frame69",
                "-i",
                "1000",
                "--mode",
                "regularized",
                "--load_ply",
                "output/walking/frame60/point_cloud/iteration_1000/point_cloud.ply",
            ]
        },
    ]
}