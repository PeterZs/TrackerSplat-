{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "init 1st frame",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplat.initialize",
            "console": "integratedTerminal",
            "args": [
                "-d",
                "data/coffee_martini/frame1",
                "--initializer",
                "dust3r",
            ]
        },
        {
            "name": "train 1st frame",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplat.train",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "data/coffee_martini/frame1",
                "-d",
                "output/coffee_martini/frame1",
                "-i",
                "1000",
            ],
            "justMyCode": false
        },
        {
            "name": "dot motionestimation",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.motionestimation",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/coffee_martini",
                "-d",
                "output/coffee_martini",
                "-i",
                "1000",
                "--estimator",
                "dot-cotracker3",
                "--iteration_init",
                "1000",
                "-n",
                "3",
                "-b",
                "5",
                "--load_camera",
                "output/coffee_martini/frame1/cameras.json",
                "--tracking_rescale",
                "0.25",
            ]
        },
        {
            "name": "sam2 featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--load_camera",
                "output/truck/cameras.json",
                "--extractor",
                "sam2",
                "--extractor_configfile",
                "./configs/sam2.1/sam2.1_hiera_l.yaml",
                "--extractor_checkpoint",
                "./checkpoints/sam2.1_hiera_large.pt",
                "--save_featuremap",
                "--colorify_algo",
                "kmeans-pca",
                "--colorify_alpha",
                "1.0",
            ]
        },
        {
            "name": "dinov2 featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--load_camera",
                "output/truck/cameras.json",
                "--extractor",
                "dinov2",
                "--extractor_configfile",
                "./configs/dinov2/vits14_reg4_pretrain.yaml",
                "--extractor_checkpoint",
                "./checkpoints/dinov2_vits14_reg4_pretrain.pth",
                "--save_featuremap",
                "--colorify_algo",
                "kmeans-pca",
                "--colorify_alpha",
                "0.8",
            ]
        },
        {
            "name": "dinov2seg featurefusion",
            "type": "debugpy",
            "request": "launch",
            "module": "instantsplatstream.featurefusion",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--load_camera",
                "output/truck/cameras.json",
                "--extractor",
                "dinov2seg",
                "--extractor_configfile",
                "giant",
                "--save_featuremap",
                "--colorify_algo",
                "weightedsum",
            ]
        },
        {
            "name": "point track debug",
            "type": "debugpy",
            "request": "launch",
            "program": "debug/point_track.py",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "data/coffee_martini",
                "-d",
                "output/coffee_martini",
                "--estimator",
                "dot-cotracker3",
                "-n",
                "3",
                "--tracking_rescale",
                "0.5",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
    ]
}