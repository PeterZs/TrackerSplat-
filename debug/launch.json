{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "motion fusion test",
            "type": "debugpy",
            "request": "launch",
            "program": "debug/motion_fusion_test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/coffee_martini",
                "-d",
                "output/coffee_martini/frame1",
                "-i",
                "1000",
                "--mode",
                "densify",
                "--load_camera",
                "output/coffee_martini/frame1/cameras.json",
                "--tracking_rescale",
                "0.25",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}